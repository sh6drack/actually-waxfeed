+      {/* "your taste. mapped." text */}
+      <div
+        style={{
+          position: "absolute",
+          bottom: isVertical ? "8%" : "10%",
+          left: 0,
+          right: 0,
+          display: "flex",
+          justifyContent: "center",
+          opacity: textOpacity,
+        }}
+      >
+        <h3
+          style={{
+            color: "#fff",
+            fontSize: isVertical ? 28 : 22,
+            fontFamily: "system-ui",
+            fontWeight: 500,
+            margin: 0,
+          }}
+        >
+          your taste. mapped.
+        </h3>
+      </div>
+    </AbsoluteFill>
+  );
+};
