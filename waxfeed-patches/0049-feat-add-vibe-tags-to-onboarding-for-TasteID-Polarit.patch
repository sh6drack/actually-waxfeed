+          />
+
+          {/* Milestone markers */}
+          {JOURNEY_MILESTONES.map((milestone, i) => {
+            const position = (i / (JOURNEY_MILESTONES.length - 1)) * 100;
+            const isReached = position / 100 <= timelineProgress;
+            const isActive = i === currentMilestoneIndex;
+
+            return (
+              <div
+                key={milestone.month}
+                style={{
+                  position: "absolute",
+                  left: `${position}%`,
+                  top: "50%",
+                  transform: "translate(-50%, -50%)",
