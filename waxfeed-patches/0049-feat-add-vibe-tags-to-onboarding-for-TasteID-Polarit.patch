+    extrapolateLeft: "clamp",
+    extrapolateRight: "clamp",
+  });
+
+  // Phase 2: Timeline reveals (frames 40-200)
+  const timelineProgress = interpolate(frame, [40, 200], [0, 1], {
+    extrapolateLeft: "clamp",
+    extrapolateRight: "clamp",
+    easing: Easing.bezier(0.25, 0.1, 0.25, 1),
