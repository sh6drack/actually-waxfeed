+            />
+            <span
+              style={{
+                fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+                fontSize: 10,
+                fontWeight: 500,
+                color: COLORS.gray[500],
+                letterSpacing: "0.05em",
+              }}
+            >
+              {type.name} ({type.minScore}%+)
+            </span>
+          </div>
+        ))}
+      </div>
+    </AbsoluteFill>
+  );
+}
+
+function StatBadge({
+  icon,
+  label,
+  count,
+  color,
+}: {
+  icon: string;
+  label: string;
+  count: number;
+  color: string;
