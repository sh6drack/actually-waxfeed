+        <StatBox
+          label="REVIEWS"
+          value={displayReviews}
+          suffix=""
+          color={COLORS.white}
+          opacity={statsProgress}
+        />
+        <StatBox
+          label="GENRES"
+          value={displayGenres}
+          suffix=""
+          color="#c44dff"
+          opacity={statsProgress}
+        />
+        <StatBox
+          label="MONTHS"
+          value={6}
+          suffix=""
+          color="#00ff88"
+          opacity={statsProgress}
+        />
+      </div>
+
+      {/* Archetype transformation animation */}
+      {transformProgress > 0 && (
+        <div
+          style={{
+            position: "absolute",
+            right: "10%",
+            top: "50%",
+            transform: "translateY(-50%)",
+            textAlign: "center",
+            opacity: transformProgress,
+          }}
+        >
+          <p
+            style={{
+              fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+              fontSize: 10,
+              fontWeight: 500,
+              color: COLORS.gray[500],
+              letterSpacing: "0.2em",
+              textTransform: "uppercase",
+              margin: 0,
+            }}
+          >
+            CURRENT ARCHETYPE
+          </p>
+          <p
+            style={{
+              fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+              fontSize: 28,
+              fontWeight: 700,
+              color: "#c44dff",
+              margin: "12px 0 0",
+            }}
+          >
+             Genre Fluid
+          </p>
+        </div>
