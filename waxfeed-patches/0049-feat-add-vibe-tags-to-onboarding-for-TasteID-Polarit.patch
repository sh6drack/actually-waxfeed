+  return (
+    <div
+      style={{
+        display: "flex",
+        alignItems: "center",
+        gap: 12,
+        padding: "10px 16px",
+        border: `1px solid ${color}40`,
+        backgroundColor: `${color}10`,
+      }}
+    >
+      <span style={{ fontSize: 20 }}>{icon}</span>
+      <div>
+        <p
+          style={{
+            fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+            fontSize: 10,
+            fontWeight: 500,
+            color: COLORS.gray[500],
+            letterSpacing: "0.1em",
+            textTransform: "uppercase",
+            margin: 0,
+          }}
+        >
+          {label}
+        </p>
+        <p
+          style={{
+            fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+            fontSize: 24,
+            fontWeight: 700,
+            color,
+            margin: "4px 0 0",
