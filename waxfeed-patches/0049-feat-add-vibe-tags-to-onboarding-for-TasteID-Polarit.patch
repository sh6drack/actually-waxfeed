+            >
+              <span
+                style={{
+                  fontFamily: "Helvetica Neue, Helvetica, Arial, sans-serif",
+                  fontSize: 12,
+                  fontWeight: 600,
+                  color: genre.color,
+                  letterSpacing: "0.05em",
+                  textTransform: "uppercase",
+                }}
+              >
+                {genre.name}
+              </span>
+            </div>
