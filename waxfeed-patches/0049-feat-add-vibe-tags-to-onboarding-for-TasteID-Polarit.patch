+
+          return (
+            <div
+              key={genre.name}
+              style={{
+                padding: "8px 16px",
+                borderRadius: 20,
+                border: `1px solid ${genre.color}`,
+                backgroundColor: `${genre.color}20`,
+                opacity: genreOpacity,
