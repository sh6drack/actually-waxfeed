+        </div>
+      </div>
+
+      {/* Genre discovery bubbles */}
+      <div
+        style={{
+          position: "absolute",
+          bottom: "25%",
+          left: 0,
+          right: 0,
+          display: "flex",
+          justifyContent: "center",
+          gap: 20,
+          flexWrap: "wrap",
+          padding: "0 10%",
+        }}
+      >
+        {GENRE_DISCOVERIES.map((genre) => {
+          const genreOpacity = interpolate(frame, [genre.frame, genre.frame + 30], [0, 1], {
